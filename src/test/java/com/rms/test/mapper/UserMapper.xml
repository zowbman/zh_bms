<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.rms.test.mapper.UserMapper">
	<resultMap type="com.rms.test.model.po.User" id="baseResultMap">
		<id column="id" property="id"/>
		<result column="userAccount" property="username"/>
	</resultMap>
	<select id="findAll" resultMap="baseResultMap">
		SELECT
			*
		FROM
			t_user
	</select>
</mapper>